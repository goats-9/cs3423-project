# Functions in Tabulate #
fun addNumbers(a, b) {
    let result = ADD(a, b);
    return result;
}

fun findMax(arr) {
    let maxVal = -10000;
    let rangeLength = LENGTH(arr);
    let currIdx = 0;

    while(currIdx < rangeLength) {
        if(arr.get(currIdx) > maxVal) {
            maxVal = arr.get(currIdx);
        }
        currIdx = ADD(currIdx, 1);
    }
    
    return maxVal;
}
